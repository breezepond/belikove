IF DB_ID ('DATABASE2019') IS NOT NULL
      DROP DATABASE EDUBASE2019;
      CREATE DATABASE EDUBASE2019;  
ON     
(NAME='EDUBASE2019',
FILENAME='D:\EDUBASE2019.mdf'
)
LOG ON
(NAME='EDUBASE2019',
FILENAME='E:\EDUBASE2019.ldf', 
IF OBJECT_ID('tb_Patient')IS NULL
  CREATE TABLE tb_Patient;
GO
CREATE TABLE tb_Patient
(
No
 VARCHAR(20)
 NOT NULL
 PRIMARY KEY(NO）,
NAME
 CHAR(20)
 NOT NULL,
SYMPTOM
 CHAR(20)
 NOT NULL,
TABOO MEDICINE
 CHAR(20),
DOCTOR
 CHAR(20)
 NOT NULL
)
IF OBJECT_ID('tb_Nurse')IS NULL
  CREATE TABLE tb_Nurse;
GO
CREATE TABLE tb_Nurse
（
No
  VARCHAR(20)
  NOT NULL
  PRIMARY KEY(No）,
NAME
  CHAR(20)
  NOT NULL,
JOB 
  CHAR(20)
  NOT NULL
）
